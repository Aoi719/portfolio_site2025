{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@base/*": ["src/base/*"],
      "@settings/*": ["src/settings/*"],
      "@layouts/*": ["src/layouts/*"],
      "@mixins/*": ["src/mixins/*"],
      "@styles/*": ["src/styles/*"]
    }
  }
}